<%= form_with(model: [:admin, @user], local: true) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    
    <%= f.label :admin %>
    <%= f.check_box :admin %>

    <%= f.label :password %>
    <%= f.password_field :password %>    
    
    <%= f.submit 'Create User' %>
  <% end %>
  